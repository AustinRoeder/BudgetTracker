@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link href="~/Template/css/app-green.css" rel="stylesheet" />
    <link href="~/Template/css/vendor.css" rel="stylesheet" />
    <link href="~/Scripts/angular-ui-switch.css" rel="stylesheet" />
    <link href="~/Content/nv.d3.min.css" rel="stylesheet" />
    <link href="~/Content/trNgGrid.min.css" rel="stylesheet" />
</head>
<body class="page-header-fixed page-quick-sidebar-over-content">
    <div ng-app="budget_tracker">
        <div id="app-container" ng-controller="layoutCtrl as layout">
            <div ng-hide="layout.$state.includes('signin')">
                <nav class="navbar navbar-inverse navbar-fixed-top striped-bg" id="top-navbar">
                    <div class="container-fluid">
                        <nav id="header" class="header navbar navbar-fixed-top navbar-inverse" role="navigation">
                            <!-- begin container-fluid -->
                            <div class="container-fluid">
                                <!-- begin mobile sidebar expand / collapse button -->
                                <div class="navbar-header">
                                    <a class="navbar-brand" ui-sref="dashboard"><i class="fa fa-paper-plane"></i>&nbsp;Budget Planner</a>
                                    <button type="button" class="navbar-toggle"
                                            ng-init="navCollapsed = true"
                                            ng-click="navCollapsed = !navCollapsed">
                                        <span class="sr-only">Toggle navigation</span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                </div>
                                <!-- end mobile sidebar expand / collapse button -->
                                <!-- begin header navigation right -->
                                <div class="collapse navbar-collapse striped-bg" style="width: 270px;background-color: #2e2e2e;" ng-class="!navCollapsed && 'in'" ng-click="navCollapsed=true">
                                    <ul class="nav navbar-nav hidden-md hidden-lg hidden-sm">
                                        <li>
                                            <a ui-sref="dashboard"><i class="fa fa-dashboard"></i> Dashboard</a>
                                        </li>
                                        <li>
                                            <a ui-sref="household.details"><i class="fa fa-home"></i> Household</a>
                                        </li>
                                        <li>
                                            <a ui-sref="accounts.list"><i class="fa fa-bank"></i> Accounts</a>
                                        </li>
                                        <li>
                                            <a ui-sref="budget.list"><i class="fa fa-money"></i> Budget</a>
                                        </li>
                                        <li>
                                            <a href ng-click="layout.logout()"><i class="fa fa-power-off"></i> Logout</a>
                                        </li>
                                    </ul>
                                </div>
                                <!-- end header navigation right -->
                            </div>
                            <!-- end container-fluid -->
                        </nav><!-- /header -->
                    </div>
                </nav> <!-- navbar top -->
                <div class="striped-bg" id="sidenav">
                    <div role="tabpanel" id="navTabs">
                        <div class="sidebar-controllers">
                            <div class="tab-content-scroller tab-content sidebar-section-wrap ps-container ps-active-y" data-ps-id="17d9a2bb-14e9-dee3-dd4a-8f1da957b4c1">
                                <ul class="nav sidebar-nav ">
                                    <li>
                                        <a ui-sref="dashboard"><i class="fa fa-dashboard"></i> Dashboard</a>
                                    </li>
                                    <li>
                                        <a ui-sref="household.details"><i class="fa fa-home"></i> House</a>
                                    </li>
                                    <li>
                                        <a ui-sref="accounts.list"><i class="fa fa-bank"></i> Accounts</a>
                                    </li>
                                    <li>
                                        <a ui-sref="budget.list"><i class="fa fa-money"></i> Budget</a>
                                    </li>
                                    <li>
                                        <a href ng-click="layout.logout()"><i class="fa fa-power-off"></i> Logout</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div> <!-- navbar side -->
            </div>
            <div ng-hide="layout.$state.includes('signin')">
                <div ui-view class="content-wrapper"></div>
            </div>
            <div ng-show="layout.$state.includes('signin')">
                <div ui-view></div>
            </div>
        </div><!-- layout -->
    </div>

    <script src="~/Scripts/d3/d3.min.js"></script>
    <script src="~/Scripts/nv.d3.min.js"></script>

    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-ui-router.min.js"></script>
    <script src="~/Scripts/angular-local-storage.min.js"></script>
    <script src="~/Scripts/angular-ui-switch.min.js"></script>
    <script src="~/Scripts/angular-ui/ui-bootstrap.min.js"></script>
    <script src="~/Scripts/angular-ui/ui-bootstrap-tpls.min.js"></script>
    <script src="~/Scripts/angular-nvd3.min.js"></script>
    <script src="~/Scripts/trNgGrid.min.js"></script>

    <script src="~/app/app.js"></script>
    <script src="~/app/services/authSvc.js"></script>
    <script src="~/app/services/authIntercepterSvc.js"></script>

    <script src="~/app/services/accountSvc.js"></script>
    <script src="~/app/services/budgetSvc.js"></script>
    <script src="~/app/services/categorySvc.js"></script>
    <script src="~/app/services/householdSvc.js"></script>
    <script src="~/app/services/transactionSvc.js"></script>
    <script src="~/app/services/budgetSvc1.js"></script>
    <script src="~/app/controllers/layoutCtrl.js"></script>
    <script src="~/app/controllers/signinCtrl.js"></script>
    <script src="~/app/controllers/dashboardCtrl.js"></script>
    <!-- Household Controllers -->
    <script src="~/app/controllers/household/houseCtrl.js"></script>
    <script src="~/app/controllers/household/houseJoinCtrl.js"></script>
    <script src="~/app/controllers/household/houseCreateCtrl.js"></script>
    <script src="~/app/controllers/household/houseDetailsCtrl.js"></script>

    <!-- Accounts Controllers -->
    <script src="~/app/controllers/accounts/accountCtrl.js"></script>
    <script src="~/app/controllers/accounts/accountListCtrl.js"></script>
    <script src="~/app/controllers/accounts/accountDetailsCtrl.js"></script>

    <!-- Budget Controllers -->
    <script src="~/app/controllers/budget/budgetCtrl.js"></script>
    <script src="~/app/controllers/budget/budgetListCtrl.js"></script>
</body>
</html>
